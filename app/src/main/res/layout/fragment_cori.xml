<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fundo"
    tools:context=".ui.cori.CoriFragment">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="101dp"
                    android:layout_height="75dp"
                    android:layout_marginTop="20dp"
                    android:src="@drawable/ufopa"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.506"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageButton
                    android:id="@+id/acao_cori"
                    android:layout_width="108dp"
                    android:layout_height="86dp"
                    android:backgroundTint="#D9F1BD"
                    android:scaleType="fitCenter"
                    android:src="@drawable/acao2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.31"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="AÇÕES"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/acao_cori"
                    app:layout_constraintStart_toStartOf="@+id/acao_cori"
                    app:layout_constraintTop_toBottomOf="@+id/acao_cori" />

                <ImageButton
                    android:id="@+id/video"
                    android:layout_width="108dp"
                    android:layout_height="86dp"
                    android:backgroundTint="#D9F1BD"
                    android:contentDescription="TODO"
                    android:scaleType="fitCenter"
                    android:src="@drawable/video2"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.844"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.31"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="VIDEOS"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/video"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="@+id/video"
                    app:layout_constraintTop_toBottomOf="@+id/video" />

                <ImageButton
                    android:id="@+id/dados_cori"
                    android:layout_width="108dp"
                    android:layout_height="86dp"
                    android:backgroundTint="#D9F1BD"
                    android:contentDescription="TODO"
                    android:scaleType="fitCenter"
                    android:src="@drawable/dados"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.844"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.599"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="DADOS"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/dados_cori"
                    app:layout_constraintStart_toStartOf="@+id/dados_cori"
                    app:layout_constraintTop_toBottomOf="@+id/dados_cori" />

                <ImageButton
                    android:id="@+id/editais_cori"
                    android:layout_width="108dp"
                    android:layout_height="86dp"
                    android:backgroundTint="#D9F1BD"
                    android:contentDescription="TODO"
                    android:scaleType="fitCenter"
                    android:src="@drawable/edital"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.599"
                    tools:ignore="SpeakableTextPresentCheck" />


                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="EDITAIS"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/editais_cori"
                    app:layout_constraintStart_toStartOf="@+id/editais_cori"
                    app:layout_constraintTop_toBottomOf="@+id/editais_cori" />

                <ImageButton
                    android:id="@+id/relatorio_cori"
                    android:layout_width="108dp"
                    android:layout_height="86dp"
                    android:layout_marginTop="444dp"
                    android:layout_marginBottom="50dp"
                    android:backgroundTint="#D9F1BD"
                    android:contentDescription="TODO"
                    android:scaleType="fitCenter"
                    android:src="@drawable/relatorio"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="RELATORIOS"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/relatorio_cori"
                    app:layout_constraintStart_toStartOf="@+id/relatorio_cori"
                    app:layout_constraintTop_toBottomOf="@+id/relatorio_cori" />

                <ImageButton
                    android:id="@+id/contato_cori"
                    android:layout_width="108dp"
                    android:layout_height="80sp"
                    android:layout_marginTop="436dp"
                    android:layout_marginBottom="50dp"
                    android:backgroundTint="#D9F1BD"
                    android:scaleType="fitCenter"
                    android:src="@drawable/contato"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.871"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="SpeakableTextPresentCheck" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="CONTATO"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="@+id/contato_cori"
                    app:layout_constraintHorizontal_bias="0.555"
                    app:layout_constraintStart_toStartOf="@+id/contato_cori"
                    app:layout_constraintTop_toBottomOf="@+id/contato_cori" />
        </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>